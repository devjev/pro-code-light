{
  "name": "Pro Code (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#b9b9b9",
    "editor.foreground": "#000000",
    "editor.foldBackground": "#a0a0a0",
    "editor.lineHighlightBackground": "#b7b7b7",
    "editor.selectionBackground": "#9a9a9a",
    "editorGroupHeader.tabsBackground": "#969696",
    "editorGutter.addedBackground": "#356d53",
    "editorGutter.deletedBackground": "#da1a32",
    "editorGutter.modifiedBackground": "#3a3a3a",
    "editorLineNumber.foreground": "#5d5d5d",
    "editorCursor.foreground": "#1362ff",
    "activityBarBadge.background": "#212121",
    "activityBar.background": "#969696",
    "activityBar.foreground": "#000000",
    "activityBar.inactiveForeground": "#575757",
    "sideBarTitle.foreground": "#000000",
    "sideBar.background": "#969696",
    "statusBar.background": "#80847f",
    "statusBar.foreground": "#323232",
    "tab.inactiveBackground": "#777777",
    "tab.inactiveForeground": "#000000",
    "tab.border": "#656565",
    "tab.hoverBackground": "#1362ff",
    "tab.hoverForeground": "#ffffff",
    "tab.activeBorderTop": "#1362ff",
    "titleBar.activeBackground": "#b9b9b9",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveBackground": "#969696",
    "titleBar.inactiveForeground": "#777777",
    "sideBar.foreground": "#323232",
    "gitDecoration.ignoredResourceForeground": "#6a6a6a",
    "activityBar.activeBackground": "#acacac",
    "list.hoverBackground": "#1362ff",
    "list.hoverForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#a0a0a0",
    "terminal.ansiCyan": "#005479",
    "terminal.ansiBrightCyan": "#00799f",
    "terminal.ansiBlue": "#002dba",
    "terminal.ansiBrightBlue": "#1362ff",
    "terminal.ansiMagenta": "#940049",
    "terminal.ansiBrightMagenta": "#b60064",
    "terminal.ansiRed": "#990006",
    "terminal.ansiBrightRed": "#a63442",
    "terminal.ansiYellow": "#5e4000",
    "terminal.ansiBrightYellow": "#805c00",
    "terminal.ansiGreen": "#006140",
    "terminal.ansiBrightGreen": "#007d5a",
    "scrollbar.shadow": "#777777",
    "editorGroup.border": "#a0a0a0",
    "debugConsole.infoForeground": "#006140",
    "debugConsole.errorForeground": "#a63442",
    "debugConsole.sourceForeground": "#000000",
    "debugConsole.warningForeground": "#a63442"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0b7b47"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#da1a32"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#3535b8"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#3535b8"
      }
    },
    {
      "name": "Keyword Punctuation",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "name": "Use stuff",
      "scope": ["keyword.other"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#101010"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000000"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": ["variable.parameter", "keyword.other.unit", "keyword.other"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#000000"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#000000"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000000"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#000000"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000000"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000000"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#000000"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#006954"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#454e56"
      }
    },
    {
