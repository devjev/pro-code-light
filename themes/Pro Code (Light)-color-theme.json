{
  "name": "Pro Code (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#DEE0E0",
    "editor.foreground": "#25282A",
    "editor.foldBackground": "#A2AAAD", // maybe something else here
    "editor.lineHighlightBackground": "#D9DBDB",
    "editor.selectionBackground": "#A2AAAD",
    "editorGroupHeader.tabsBackground": "#A2AAAD",
    "editorGroup.border": "#A2AAAD",
    "editorGutter.addedBackground": "#85a696",
    "editorGutter.deletedBackground": "#da1a32",
    "editorGutter.modifiedBackground": "#d9bd8d",
    "editorLineNumber.foreground": "#5d5d5d",
    "editorCursor.foreground": "#000000",
    "activityBarBadge.background": "#485CC7",
    "activityBar.background": "#A2AAAD",
    "activityBar.foreground": "#25282A",
    "activityBar.inactiveForeground": "#7C878E",
    "activityBar.activeBackground": "#A2AAAD",
    "sideBarTitle.foreground": "#25282A",
    "sideBar.background": "#A2AAAD",
    "sideBar.foreground": "#25282A",
    "statusBar.background": "#A2AAAD",
    "statusBar.foreground": "#25282A",
    "statusBar.noFolderBackground": "#A2AAAD",
    "statusBar.noFolderForeground": "#25282A",
    "statusBar.debuggingBackground": "#651C32",
    "statusBar.debuggingForeground": "#D9DBDB",
    "tab.inactiveBackground": "#7C878E",
    "tab.inactiveForeground": "#25282A",
    "tab.border": "#7C878E",
    "tab.hoverBackground": "#1362ff",
    "tab.hoverForeground": "#ffffff",
    "tab.activeBorderTop": "#1362ff",
    "titleBar.activeBackground": "#7C878E",
    "titleBar.activeForeground": "#25282A",
    "titleBar.inactiveBackground": "#A2AAAD",
    "titleBar.inactiveForeground": "#7C878E",
    "gitDecoration.ignoredResourceForeground": "#6a6a6a",
    "list.hoverBackground": "#1362ff",
    "list.hoverForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#A2AAAD",
    "terminal.ansiCyan": "#005479",
    "terminal.ansiBrightCyan": "#00799f",
    "terminal.ansiBlue": "#002dba",
    "terminal.ansiBrightBlue": "#1362ff",
    "terminal.ansiMagenta": "#940049",
    "terminal.ansiBrightMagenta": "#b60064",
    "terminal.ansiRed": "#990006",
    "terminal.ansiBrightRed": "#a63442",
    "terminal.ansiYellow": "#5e4000",
    "terminal.ansiBrightYellow": "#805c00",
    "terminal.ansiGreen": "#006140",
    "terminal.ansiBrightGreen": "#007d5a",
    "scrollbar.shadow": "#7C878E",
    "debugConsole.infoForeground": "#006140",
    "debugConsole.errorForeground": "#a63442",
    "debugConsole.sourceForeground": "#25282A",
    "debugConsole.warningForeground": "#a63442"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00573F"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#da1a32"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#004FB5"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#004FB5"
      }
    },
    {
      "name": "Keyword Punctuation",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#0c4c1c"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#0c4c1c"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#25282A"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#25282A"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "CSS ID's",
      "scope": [
        "source.sass keyword.control"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#25282A"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#25282A"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
